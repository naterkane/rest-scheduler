#%RAML 0.8
title: rest-agendaw
version: 0.1.0
mediaType: application/json
baseUri: /
documentation:
 - title: Getting Started
   content: !include ../README.md
resourceTypes:
  - callback:
      description: Entity representing a read-only <<resourcePathName|!singularize>>
      post:
        description: |
          Test the <<resourcePathName|!singularize>>
          with <<resourcePathName|!singularize>>Id =
          {<<resourcePathName|!singularize>>Id}
          If called anonymously, the <<resourcePathName|!singularize>>
          must be public.
        body:
          application/json:
            schema: |
              <<schemaItem>>
              #<<resourcePathName|!singularize>>
            example: |
              <<exampleItem>>
        responses:
          200:
            body:
              application/json:
                example: |
                  <<exampleItem>>
                schema: <<schemaItem>>
  - collection:
      is: [versionable]
      securedBy: [null]
      description: Collection of available <<resourcePathName>> in REST-Scheduler.
      get:
        description: |
          Get a list of <<resourcePathName>>.  If called anonymously,
          this will return all public <<resourcePathName>>.
        responses:
          200:
            body:
              application/json:
                example: |
                  <<exampleCollection>>
                schema: <<schemaCollection>>
      post:
        description: |
          Add a new <<resourcePathName|!singularize>> to REST-Scheduler.
        queryParameters:
          access_token:
            description: "The access token provided by the authentication application"
            example: AABBCCDD
            required: true
            type: string
        body:
          application/json:
            schema: <<resourcePathName|!singularize>>
            example: |
              <<exampleItem>>
        responses:
          200:
            body:
              application/json:
                example:
  - create-job:
      description: Create an entity representing a <<resourcePathName|!singularize>>
      post:
        description: |
          Add a new <<resourcePathName|!singularize>> to REST-Scheduler.
        headers:
          Content-Type:
            description: the required content type and encoding
            type: string
            example: |
              application/json; charset=utf-8
        body:
          application/json:
            schema: |
              <<schemaItem>>
              #<<resourcePathName|!singularize>>
            example: |
              <<exampleItem>>
        responses:
          200:
            headers:
              Content-Type:
                description: the required content type and encoding
                type: string
                example: |
                  application/json; charset=utf-8
            body:
              application/json:
                example: <<exampleItem>>
                schema: <<schemaItem>>
          400:
            headers:
              Content-Type:
                description: the required content type and encoding
                type: string
                example: |
                  application/json; charset=utf-8
            body:
              application/json:
                example: |
                  {"message": "We could not process your request <<resourcePathName|!singularize>>" }
          500:
            headers:
              Content-Type:
                description: the required content type and encoding
                type: string
                example: |
                  application/json; charset=utf-8
            body:
              application/json:
                example: |
                  {"message": "Job <<resourcePathName|!singularize>> could not be created" }
  - job:
      description: Entity representing a <<resourcePathName|!singularize>>
      get:
        description: |
          Get the <<resourcePathName|!singularize>>
          with <<resourcePathName|!singularize>>Id =
          {<<resourcePathName|!singularize>>Id}
        headers:
          Content-Type:
            description: the required content type and encoding
            type: string
            example: |
              application/json; charset=utf-8
        responses:
          200:
            headers:
              Content-Type:
                description: the required content type and encoding
                type: string
                example: |
                  application/json; charset=utf-8
            body:
              application/json:
                example: <<exampleItem>>
                schema: <<schemaItem>>
          400:
            headers:
              Content-Type:
                description: the required content type and encoding
                type: string
                example: |
                  application/json; charset=utf-8
            body:
              application/json:
                example: |
                  {"message": "<<resourcePathName|!singularize>> not found" }
      put:
        description: |
          Update the <<resourcePathName|!singularize>>
          with <<resourcePathName|!singularize>>Id =
          {<<resourcePathName|!singularize>>Id}.
        headers:
          Content-Type:
            description: the required content type and encoding
            type: string
            example: |
              application/json; charset=utf-8
        body:
          application/json:
            schema: <<schemaItem>>
            example: <<exampleItem>>
        responses:
          200:
            headers:
              Content-Type:
                description: the required content type and encoding
                type: string
                example: |
                  application/json; charset=utf-8
            body:
              application/json:
                example: <<exampleItem>>
                schema: <<schemaItem>>
          400:
            headers:
              Content-Type:
                description: the required content type and encoding
                type: string
                example: |
                  application/json; charset=utf-8
            body:
              application/json:
                example: |
                  {"message": "Job <<resourcePathName|!singularize>> not found" }

traits:
- versionable:
      headers:
        Accept-Version:
          description: the version that is acceptable according to semver.org matching
          type: string
          example: ~1
      responses:
        200:
          headers:
            Version:
              description: the (semver) version provided
              type: string
              example: 1.0.6


